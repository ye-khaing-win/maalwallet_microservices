{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/admin-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/admin-gateway/tsconfig.app.json",
    "assets": ["**/*.proto"],
    "watchAssets": true
  },
  "monorepo": true,
  "root": "apps/admin-gateway",
  "projects": {
    "admin-gateway": {
      "type": "application",
      "root": "apps/admin-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/admin-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-gateway/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    },
    "tokens-service": {
      "type": "application",
      "root": "apps/tokens-service",
      "entryFile": "main",
      "sourceRoot": "apps/tokens-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tokens-service/tsconfig.app.json"
      }
    }
  }
}